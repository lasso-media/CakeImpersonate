{
    "name": "lasso-media/cake-impersonate",
    "description": "Impersonate plugin for CakePHP 4",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0.1"
    },
    "autoload": {
        "psr-4": {
            "CakeImpersonate\\": "src/",
            "CakeImpersonate\\Test\\Fixture\\": "tests/Fixture/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeImpersonate\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "Cake\\PHPStan\\": "vendor/cakephp/cakephp/tests/PHPStan/",
            "App\\": "tests/test_app/"
        }
    },
    "config": {
        "process-timeout": 600,
        "sort-packages": true
    },
    "prefer-stable": true
}
